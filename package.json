{
  "name": "algorithms",
  "version": "1.0.0",
  "description": "algorithms",
  "license": "MIT",
  "repository": "",
  "author": {
    "name": "Michael Heuss",
    "email": "6349935+mheuss@users.noreply.github.com",
    "url": ""
  },
  "keywords": [
    "Algorithms",
    "Stacks",
    "Heaps",
    "Queues",
    "Sorts",
    "Diffing",
    "Linked Lists",
    "Trees"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage && rimraf docs",
    "docs": "typedoc --excludeProtected --excludePrivate --excludeNotExported --mode file --exclude \"**/**+(.spec).ts\" --exclude \"**/mock**\" --out docs src",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\" --single-quote --trailing-comma es5",
    "lint": "eslint  ./src",
    "lint:fix": "eslint --fix ./src",
    "neaten": "npm run lint:fix",
    "pre-commit": "npm run lint && npm run test",
    "build": "tsc --pretty",
    "test": "jest",
    "coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "object-hash": "^3.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.153",
    "@types/node": "^18.7.23",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "coveralls": "^3.0.2",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prepush"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  }
}
